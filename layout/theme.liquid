<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <link rel="canonical" href="{{ canonical_url }}">
  {% render 'social-meta-tags.liquid' %}
  <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
  {{ content_for_header }} <!-- Header hook for plugins -->
  <!-- og meta tags -->
  <!-- <meta property="og:title" content="Verbeek's Farm & Garden Centre" />
  <meta name="description" property="og:description" content="Verbeek's dates back to the year 1970. We have grown to service everyone from the home gardener to the largest farm properties across the county." />
  <meta property="og:url" content="https//www.verbeekslandscaping.com" />
  <meta name="image" property="og:image" content="{{ 'favicon.png' | asset_url }}" />  -->
  <!-- Favicon -->
  <link type="image/png" href="{{ 'favicon.png' | asset_url }}" rel="icon">
  <!-- Font Awesome -->
  <script src="https://kit.fontawesome.com/d9461c25c6.js"></script>
  <!-- Flickity -->
  <link rel="stylesheet" href="https://unpkg.com/flickity@2/dist/flickity.min.css">
  <!-- CSS -->
  {{ 'application.scss.css' | asset_url | stylesheet_tag }}
  <title>{{ page_title }}</title>
</head>
<body>

  {% section 'header' %}

  <main role="main">
    {{ content_for_layout }}
  </main>

  {% section 'footer' %}
</body>
  <!-- jQuery -->
  <script src="https://code.jquery.com/jquery-3.4.1.js" integrity="sha256-WpOohJOqMqqyKL9FccASB9O0KwACQJpFTUBLTYOVvVU="
    crossorigin="anonymous"></script>
  <!-- JavaScript -->
  {{ 'application.js' | asset_url | script_tag }}
  <script src="https://unpkg.com/flickity@2/dist/flickity.pkgd.min.js"></script>
</html>
