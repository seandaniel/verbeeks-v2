 <section class="landscape design">
  <div class="landscaping-header-text">
    <p>{{ section.settings.landscape_design_header }}</p>
    <div class="svg-container">
      <img src="{{ 'brick-wall.svg' | asset_url }}" alt="Brick Wall">
    </div>
  </div>
  <div class="design-img-container">
    <figure>
      <img src="{{ section.settings.landscape_design_1 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_1_alt }}">
      <figcaption>{{ section.settings.landscape_design_1_heading }}</figcaption>
    </figure>
    <figure>
      <img src="{{ section.settings.landscape_design_2 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_2_alt }}">
      <figcaption>{{ section.settings.landscape_design_2_heading }}</figcaption>
    </figure>
    <figure>
      <img src="{{ section.settings.landscape_design_3 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_3_alt }}">
      <figcaption>{{ section.settings.landscape_design_3_heading }}</figcaption>
    </figure>
    <figure>
      <img src="{{ section.settings.landscape_design_4 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_4_alt }}">
      <figcaption>{{ section.settings.landscape_design_4_heading }}</figcaption>
    </figure>
    <figure>
      <img src="{{ section.settings.landscape_design_5 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_5_alt }}">
      <figcaption>{{ section.settings.landscape_design_5_heading }}</figcaption>
    </figure>
    <figure>
      <img src="{{ section.settings.landscape_design_6 | img_url: 'master' }}" alt="{{ section.settings.landscape_design_6_alt }}">
      <figcaption>{{ section.settings.landscape_design_6_heading }}</figcaption>
    </figure>
  </div>
</section>

<section class="landscape garden" hidden>
  <img src="{{ section.settings.landscape_garden_1 | img_url: 'master' }}"
    alt="{{ section.settings.landscape_garden_1_alt }}">
</section>

<section class="landscape lawn" hidden>
  <img src="{{ section.settings.landscape_lawn_1 | img_url: 'master' }}"
    alt="{{ section.settings.landscape_lawn_1_alt }}">
</section>
 
